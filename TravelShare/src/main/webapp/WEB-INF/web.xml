<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>PostRegister</servlet-name>
  	<display-name>PostRegister</display-name>
  	<description></description>
  	<servlet-class>controler.PostRegister</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreatePost</servlet-name>
  	<display-name>CreatePost</display-name>
  	<description></description>
  	<servlet-class>controler.CreatePost</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>controller.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PostRegister</servlet-name>
  	<url-pattern>/PostRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreatePost</servlet-name>
  	<url-pattern>/CreatePost</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
</web-app>
